<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="AtMonitor.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:AtMonitor"
    Shell.FlyoutBehavior="Disabled"
    Title="AtMonitor">
    <ShellContent
        FlyoutIcon="{StaticResource CarCrashLight}"
        Title="Überwachung"
        Route="MainPage" />
    <ShellContent
        Title="Über"
        Icon="{AppThemeBinding Light={StaticResource AboutLight}, Dark={StaticResource AboutDark}}"/>
    <ShellContent
        Title="Einstllungen"
        Icon="{AppThemeBinding Light={StaticResource GearLight}, Dark={StaticResource GearDark}}"
        />
</Shell>
